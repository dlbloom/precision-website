<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <security>
            <authentication>
                <basicAuthentication enabled="true" />
            </authentication>
        </security>
        <defaultDocument>
            <files>
                <clear />
                <add value="index.aspx" />
                <add value="index.asp" />
                <add value="index.php" />
                <add value="index.pl" />
                <add value="index.py" />
                <add value="index.html" />
                <add value="index.htm" />
                <add value="default.aspx" />
                <add value="default.asp" />
                <add value="default.html" />
                <add value="default.htm" />
            </files>
        </defaultDocument>
        <httpRedirect enabled="false">
            <clear />
        </httpRedirect>
        <httpErrors errorMode="Detailed" />
    </system.webServer>
</configuration>
